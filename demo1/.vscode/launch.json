{
    "configurations": [
      {
        "name": "mingw64 build and debug",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/build/demo1.exe",
        "args": [],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
